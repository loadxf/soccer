# Sample environment variables for GCP deployment
# Copy this file to .env.gcp and update the values

# GCP Project ID
PROJECT_ID=soccer-prediction-12345

# GKE Cluster Name
GKE_CLUSTER_NAME=soccer-prediction-cluster
GKE_CLUSTER_ZONE=us-central1-a

# Cloud SQL Database Information
CLOUD_SQL_INSTANCE=soccer-prediction-db
CLOUD_SQL_REGION=us-central1

# Domain and DNS settings
DOMAIN_NAME=soccer-prediction.example.com

# Static IP name for ingress
STATIC_IP_NAME=soccer-prediction-ip 